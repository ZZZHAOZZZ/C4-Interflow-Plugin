{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}/c4interflowvisualisation"
            ],
            "name": "Launch Extension",
            "outFiles": [
                "${workspaceFolder}/c4interflowvisualisation/out/**/*.js"
            ],
            "preLaunchTask": "npm: compile",
            "request": "launch",
            "type": "extensionHost"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${file}"
        }
    ]
}